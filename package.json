{
  "name": "Treebee",
  "version": "1.0.0",
  "description": "Tool for query and process Elasticsearch responses.",
  "main": "index.js",
  "author": "juazugas <juazuri@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node server.js",
    "prototype": "node prototype/server.js",
    "prestart": "babel-node tools/start-motd.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "yarn run lint -- --watch",
    "test": "mocha --compilers js:babel-core/register --reporter spec tools/test-setup.js \"test/**/*.spec.js\"",
    "test:watch": "yarn run test -- --watch",
    "coverage": "nyc mocha --compilers js:babel-core/register --reporter min tools/test-setup.js \"test/**/*.spec.js\""
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.7",
    "jquery": "^3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "eslint": "^4.13.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "expect": "^21.2.1",
    "express": "^4.16.2",
    "jsdom": "^11.5.1",
    "mocha": "^4.0.1",
    "nock": "^9.1.4",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.3.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
